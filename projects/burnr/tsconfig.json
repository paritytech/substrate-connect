{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "lib": ["ES2015", "DOM", "DOM.Iterable", "ScriptHost"],
    "module": "ES2015",
    "target": "ES2015",
    "types": [],
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react"
  },
  "include": ["src", "."]
}
