    "@polkadot/api-augment": "^9.3.2",
    "@polkadot/keyring": "^10.1.7",
    "@polkadot/react-identicon": "^2.9.8",
    "@polkadot/rpc-provider": "^9.3.2",
    "@polkadot/types": "^9.4.1",
    "@polkadot/util": "^10.1.7",
    "@polkadot/util-crypto": "^10.1.7",
    "bn.js": "^5.2.1",
    "qrcode.react": "^3.1.0",
